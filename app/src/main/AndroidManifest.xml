<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.laptoppoint.highwayrestaurantmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mainly.SignUPActivity" />
        <activity android:name=".mainly.MainActivity" />
        <activity android:name=".mainly.OrderActivity" />
        <activity android:name=".mainly.ProductsActivity" />
        <activity android:name=".mainly.TableActivity" />
        <activity android:name=".mainly.ProductCategoriesActivity"></activity>
    </application>

</manifest>